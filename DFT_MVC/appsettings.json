{
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    },
    "DisableStringReuse": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DFT_MVC_Context": "Server=(localdb)\\mssqllocaldb;Database=DFT_MVC.Data;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}